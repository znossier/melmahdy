{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "moustafaelmahdy.framer.website"
        }
      ],
      "destination": "https://melmahdy.vercel.app/:path*",
      "permanent": true
    }
  ]
}
